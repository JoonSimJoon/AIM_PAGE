# 프로덕션 환경용 환경변수

# AWS S3 설정
AWS_REGION=ap-northeast-2
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
AWS_S3_BUCKET=your-s3-bucket-name

# CloudFront (선택사항)
CLOUDFRONT_DOMAIN=your-cloudfront-domain.cloudfront.net

# 데이터베이스 설정 (프로덕션용 - 보안 강화 필요)
POSTGRES_USER=aimpage_prod
POSTGRES_PASSWORD=super-secure-password-change-this
POSTGRES_DB=aim_page_prod

# JWT 시크릿 (프로덕션용 - 반드시 변경)
JWT_SECRET=super-secret-jwt-key-for-production-change-this

# 프론트엔드 URL
FRONTEND_URL=https://yourdomain.com
NEXT_PUBLIC_API_URL=https://api.yourdomain.com
